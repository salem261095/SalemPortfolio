---
import Layout from "@/layouts/Layout.astro";
import Hero from "@/components/astro/Hero.astro";
import WebProject from "@/components/astro/WebProject.astro";
import GraphicProject from "@/components/astro/GraphicProject.astro";
import Testimonials from "@/components/astro/testimonials.astro";
import "@/style/global.css";
import Services from "@/components/astro/Services.astro";
import CallToAction from "@/components/astro/CallToAction.astro";
import LogoCarousel from "@/components/astro/LogoCarousel.astro";
import Blogs from "@/components/astro/Blogs.astro";
import SEO from "@/components/astro/SEO.astro";
import { SiteConfig } from "@/data/config";
const AppName = SiteConfig.AppName;
---

<Layout>
  <SEO
    title="مطوّر ويب ومصمم جرافيك"
    description="سالم قوقح، مصمم جرافيك ومطوّر ويب بخبرة +8 سنوات، ساهم في تأسيس هويات بصرية ومواقع احترافية لمؤسسات ومشاريع عربية."
    url={AppName}
    image="/assets/og-image.png"
  />
  <Hero />
  <LogoCarousel />
  <Services />
  <WebProject />
  <GraphicProject />
  <Testimonials />
  <Blogs />
  <CallToAction />
</Layout>
